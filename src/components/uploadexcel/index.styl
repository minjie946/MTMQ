/** 导入错误之后的提示 start */
.cus-modal-msg-title {
  text-align center
}
.cus-modal-msg-waring {
  margin-bottom 16px;
  padding 0 16px;
  .text-info {
    font-size 11px
    line-height 22px
  }
}
.cus-modal-msg-info {
  width:358px;
  padding 20px;
  background:rgba(242,246,252,1);
  box-sizing border-box;
  p {
    font-size:11px;
    line-height 16px;
    margin-bottom 8px
    font-weight:400;
    color:rgba(245,34,45,1);
  }
  p:nth-child(1) {
    color:rgba(51,51,51,1);
  }
}
.cus-modal-msg-btn {
  text-align center
  margin-top 25px
}
/** 导入错误之后的提示 end */


/* 文件上传的(excel) start */
.upload-file-content {
  display: inline-block;
}

.upload-progress {
  width:100%;
  height:232px;
  background:rgba(255,255,255,1);
  border-radius:5px;
  text-align center
  padding 56px 0
  > p {
    height:22px;
    font-size:16px;/*no*/
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:22px;
    margin-bottom 68px
  }
}
/* 文件上传的(excel) end */

