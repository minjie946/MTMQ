.post-icon {
  display inline-block;
  width 29px;
  height:29px;
  background:rgba(255,255,255,1);
  box-shadow:0px 2px 6px 0px rgba(0,0,0,0.2);
  border-radius:3px;
  color #FF4A14;
  text-align center;
  i {
    display block;
    line-height 31px;
  }
}
